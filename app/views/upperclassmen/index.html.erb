<div class="page-header">
  <h3>Upperclassmen <span class="talpacket hidden">Tal Packet </span>Signatures</h3>
</div>

<div class="table-responsive">
  <table class="table table-bordered table-hover table-condensed">
    <thead>
      <tr>
        <th>Name</th>
        <th><span class="talpacket hidden">Tal </span>Packets Signed</th>
      </tr>
    </thead>

    <tbody>
      <% @upperclassmen.each do |u| %>
        <tr>
          <td>
            <%= link_to upperclassman_path(u[0].id) do %>
              <span class="talpacket hidden">Tal Packet - </span><%= u[0].name %>
            <% end %>
          </td>
          <td><%= u[1] %><span class="talpacket"> Tal Packets</span></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
